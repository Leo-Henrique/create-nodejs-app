{
  "name": "app-name",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "start": "node ./dist/infra/server.js",
    "start:dev": "nest start --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0 --cache",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier . --write --cache",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage.enabled=true",
    "test:integration": "vitest run --config ./vitest.config.integration.mts",
    "test:integration:watch": "vitest --config ./vitest.config.integration.mts",
    "test:integration:coverage": "vitest run --config ./vitest.config.integration.mts --coverage.enabled=true",
    "build": "nest build"
  },
  "dependencies": {
    "@anatine/zod-openapi": "2.2.8",
    "@fastify/static": "8.1.1",
    "@nestjs/common": "10.4.20",
    "@nestjs/core": "10.4.20",
    "@nestjs/platform-fastify": "10.4.20",
    "@nestjs/swagger": "7.4.2",
    "dotenv": "16.4.7",
    "fastify": "4.28.1",
    "fastify-multer": "2.0.3",
    "mime-types": "2.1.35",
    "pretty-bytes": "5.6.0",
    "rxjs": "7.8.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@faker-js/faker": "9.9.0",
    "@nestjs/cli": "10.4.9",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.20",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.14.0",
    "@types/mime-types": "2.1.4",
    "@types/node": "22.18.11",
    "@types/supertest": "6.0.3",
    "@vitest/coverage-v8": "1.6.1",
    "@vitest/eslint-plugin": "1.1.38",
    "eslint": "9.23.0",
    "eslint-config-prettier": "9.1.2",
    "globals": "15.15.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "supertest": "7.1.4",
    "type-fest": "4.38.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.28.0",
    "unplugin-swc": "1.5.7",
    "vitest": "1.6.1"
  }
}
