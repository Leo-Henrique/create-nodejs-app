{
  "name": "app-name",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "start": "node ./dist/src/infra/http-server.js",
    "start:dev": "tsx watch ./src/infra/http-server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0 --cache",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier . --write --cache",
    "prebuild": "rimraf ./dist",
    "build": "swc ./src ./package.json --copy-files --out-dir ./dist",
    "test:integration": "vitest run --config ./vitest.config.integration.mts",
    "test:integration:watch": "vitest --config ./vitest.config.integration.mts"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "11.0.1",
    "@fastify/multipart": "9.0.3",
    "@fastify/swagger": "9.4.2",
    "@fastify/swagger-ui": "5.2.3",
    "dotenv": "16.4.7",
    "fastify": "5.3.2",
    "fastify-plugin": "5.0.1",
    "fastify-type-provider-zod": "4.0.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.24.0",
    "@faker-js/faker": "9.9.0",
    "@swc/cli": "0.6.0",
    "@types/node": "22.19.7",
    "@vitest/eslint-plugin": "1.6.6",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.2",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tsx": "4.19.3",
    "type-fest": "4.39.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.1.1"
  }
}
