{
  "name": "app-name",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "start": "node ./dist/infra/server.js",
    "start:dev": "tsx watch ./src/infra/server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings 0 --cache",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier . --write --cache",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage.enabled=true",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.mts",
    "test:e2e:coverage": "vitest run --config ./vitest.config.e2e.mts --coverage.enabled=true",
    "prebuild": "rimraf ./dist",
    "build": "unbuild"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/cors": "9.0.1",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "3.0.0",
    "dotenv": "16.4.5",
    "fastify": "4.27.0",
    "fastify-multer": "2.0.3",
    "fastify-type-provider-zod": "1.2.0",
    "mime-types": "2.1.35",
    "pretty-bytes": "5.6.0",
    "zod": "3.23.8",
    "zod-validation-error": "3.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/mime-types": "2.1.4",
    "@types/node": "20.12.12",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vitest": "0.4.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.5",
    "prettier": "3.2.5",
    "rimraf": "5.0.7",
    "supertest": "7.0.0",
    "tsx": "4.11.0",
    "type-fest": "4.20.0",
    "typescript": "5.4.5",
    "unbuild": "2.0.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  }
}
